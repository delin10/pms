<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<filter>
		<filter-name>CheckResource</filter-name>
		<display-name>CheckResource</display-name>
		<description></description>
		<filter-class>pms.filter.CheckResource</filter-class>
	</filter>
	<filter>
		<filter-name>EncodeFilter</filter-name>
		<display-name>EncodeFilter</display-name>
		<description></description>
		<filter-class>pms.filter.EncodeFilter</filter-class>
	</filter>
	<filter>
		<filter-name>CrosFilter</filter-name>
		<display-name>CrosFilter</display-name>
		<description></description>
		<filter-class>pms.filter.CrosFilter</filter-class>
	</filter>
	<filter>
		<filter-name>LoginFilter</filter-name>
		<display-name>LoginFilter</display-name>
		<description></description>
		<filter-class>pms.filter.LoginFilter</filter-class>
	</filter>

	<filter-mapping>
		<filter-name>EncodeFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>CrosFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>LoginFilter</filter-name>
		<url-pattern>/setting</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>LoginFilter</filter-name>
		<url-pattern>/init</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>CheckResource</filter-name>
		<url-pattern>/setting</url-pattern>
	</filter-mapping>
	<servlet>
		<servlet-name>SystemServlet</servlet-name>
		<display-name>SystemServlet</display-name>
		<description></description>
		<servlet-class>pms.servlet.SystemServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>test</servlet-name>
		<display-name>test</display-name>
		<description></description>
		<servlet-class>pms.servlet.test</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>login</servlet-name>
		<display-name>login</display-name>
		<description></description>
		<servlet-class>pms.servlet.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>init</servlet-name>
		<display-name>InitServlet</display-name>
		<description></description>
		<servlet-class>pms.servlet.InitServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DispatcherServlet</servlet-name>
		<display-name>DispatcherServlet</display-name>
		<description></description>
		<servlet-class>pms.servlet.DispatcherServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SystemServlet</servlet-name>
		<url-pattern>/setting</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>test</servlet-name>
		<url-pattern>/test</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>init</servlet-name>
		<url-pattern>/init</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DispatcherServlet</servlet-name>
		<url-pattern>*.action</url-pattern>
	</servlet-mapping>
</web-app>
